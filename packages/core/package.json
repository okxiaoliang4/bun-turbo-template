{
  "name": "@office-suite/core",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "bun@1.1.30",
  "description": "_description_",
  "author": "jelf <okxiaoliang4@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/okxiaoliang4",
  "homepage": "https://github.com/okxiaoliang4/office-suite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okxiaoliang4/office-suite.git"
  },
  "bugs": "https://github.com/okxiaoliang4/office-suite/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": ["./dist/*", "./dist/index.d.ts"]
    }
  },
  "workspaces": ["packages/*"],
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "lint:fix": "biome check --fix .",
    "lint:ci": "biome check --ci .",
    "format:ci": "biome format --ci .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.4",
    "bumpp": "^9.5.2",
    "esno": "^4.7.0",
    "tsup": "^8.3.5",
    "turbo": "^2.2.3",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vite": "^5.4.3"
  },
  "dependencies": {
    "@pixi/ui": "^2.1.5",
    "@tanstack/virtual-core": "^3.10.8",
    "konva": "^9.3.16",
    "lodash-es": "^4.17.21",
    "type-fest": "^4.26.1"
  }
}
