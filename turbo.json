{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "lint": {},
    "format": {},
    "format:fix": {},
    "lint:fix": {},
    "lint:ci": {},
    "typecheck": {},
    "format:ci": {},
    "test": {
      "inputs": ["**/*.test.ts"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "persistent": true
    }
  }
}
